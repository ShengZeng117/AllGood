<div class="left">
    <div id="head">
        {{#with gm}}
        <h1 id="name">{{this.FirstName}} {{this.LastName}}</h1>
        <p id="position">General Department</p>
        {{/with}}
    </div>
    {{#with gm}}
    <div id="main">
        <a href="/generalmanager/{{this._id}}/personalpage">
            <div class="sub" id="overview">Overview</div>
        </a>
        <a href="/generalmanager/{{this._id}}/createAccount">
            <div class="sub" id="creatNew" href="/generalmanager/{{this._id}}/createAccount">Create Account</div>
        </a>
        <a href="/generalmanager/{{this._id}}/devices">
        <div class="sub" id="datas">Data</div>
        </a>
        <a href="/generalmanager/{{this._id}}/profile">
        <div class="sub" id="profile">Profile</div>
        </a>
        <a href="/generalmanager/{{this._id}}/staff">
        <div class="sub" id="staff">Staff</div>
        </a>
        <a href="/generalmanager/{{this._id}}/department">
        <div class="sub" id="department">Department</div>
        </a>
    </div>
    {{/with}}
    <div id="foot">
        <img src="/picture/logo.png" alt="">
    </div>
</div>
    
    <div class="content" id="prof">
        <p id="detail"> Personal Detail</p>
        <form action="" method="post">
        <div class="person">
            {{#with gm}}
            <div class="attr name">
                <div>
                    <span class="info">First name:</span>
                    <input type="text" id="firstName" name="firstName" value={{this.FirstName}}>
                    <img src="/picture/pencil-solid.png" class="changes">
                </div>
                <div id="last">
                    <span class="info">Last name:</span>
                    <input type="text" id="lastName" name="lastName" value={{this.LastName}}>
                    <img src="/picture/pencil-solid.png" class="changes">
                </div>

            </div>
            {{/with}}

            <div class="attr gender">
            <div class="dropBox">
                <span class="info">Gender</span>
                {{#with gender}}
                <select name="gender" class="genderBox" id="gender">
                    {{#if this.Male}}
                        <option value="male" selected>Male</option>
                        <option value="female">Female</option>
                        <option value="Other">Other</option>
                    {{else}}
                    {{#if this.Female}}
                        <option value="male">Male</option>
                        <option value="female" selected>Female</option>
                        <option value="Other">Other</option>
                    {{else}}
                        <option value="male">Male</option>
                        <option value="female">Female</option>
                        <option value="Other" selected>Other</option>
                    {{/if}}
                    {{/if}}
                </select>
                {{/with}}
            </div>
        </div>

            {{#with gm}}
            <div class="attr id">
                <span class="info">Staff ID:</span>
                <span class="text1"> {{this.Id}}</span>
            </div>
            <div class="attr">
                <span class="info">Email:</span>
                <span class="text2">{{this.Email}}</span>
            </div>
            <div class="attr contact">
                <span class="info">Contact number:</span>
                <input type="text" class="text3" id="Cnumber" name="Cnumber" value={{this.ContactNumber}}>
                <img src="/picture/pencil-solid.png" class="changes">
            </div>
            <div class="attr password">
                <span class="info">Password</span>
                <span class="text4"><a href="#" id="changePass">Change Password</a></span>
            </div>
            {{/with}}
            <button class="savebtn">Save Changes</button>

        </div>
        </form>
    </div>


